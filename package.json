{
	"name": "vscode-string-transform",
	"displayName": "vscode-string-transform",
	"description": "变量/字符串命名规范自由转换",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-string-transform.helloWorld_HelloWorld",
		"onCommand:vscode-string-transform.helloWorld_HELLO_WORLD",
		"onCommand:vscode-string-transform.HelloWorld_helloWorld",
		"onCommand:vscode-string-transform.HelloWorld_HELLO_WORLD",
		"onCommand:vscode-string-transform.HELLO_WORLD_helloWorld",
		"onCommand:vscode-string-transform.HELLO_WORLD_HelloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-string-transform.helloWorld_HelloWorld",
				"title": "helloWorld -> HelloWorld"
			},
			{
				"command": "vscode-string-transform.helloWorld_HELLO_WORLD",
				"title": "helloWorld -> HELLO_WORLD"
			},
			{
				"command": "vscode-string-transform.HelloWorld_helloWorld",
				"title": "HelloWorld -> helloWorld"
			},
			{
				"command": "vscode-string-transform.HelloWorld_HELLO_WORLD",
				"title": "HelloWorld -> HELLO_WORLD"
			},
			{
				"command": "vscode-string-transform.HELLO_WORLD_helloWorld",
				"title": "HELLO_WORLD -> helloWorld"
			},
			{
				"command": "vscode-string-transform.HELLO_WORLD_HelloWorld",
				"title": "HELLO_WORLD -> HelloWorld"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
